    <!-- Ice Patch 1: Straight Section -->
    <model name="ice_zone1">
      <pose>50 0 0.01 0 0 0</pose>  <!-- Slightly elevated to prevent z-fighting -->
      <link name="link">
        <collision name="ice_collision">
          <geometry>
            <box>
              <size>30 7.34 0.1</size>  <!-- Covers full road width -->
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.1</mu>    <!-- Ice-like friction -->
                <mu2>0.05</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name="ice_visual">
          <geometry>
            <box>
              <size>30 7.34 0.1</size>
            </box>
          </geometry>
          <material>
            <ambient>0.3 0.3 0.8 0.5</ambient>  <!-- Semi-transparent blue -->
          </material>
        </visual>
      </link>
      <static>true</static>
    </model>

